{
  "name": "carpe-noctem-static-site-assets",
  "version": "1.0.0",
  "description": "Static web assets generated for site using webpack to developer and publish dist artifacts.",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server -d --config webpack.config.js",
    "test": "node_modules/.bin/jest --config=jest.config.json",
    "test-update-ui-snapshots": "node_modules/.bin/jest -u --config=jest.config.json",
    "compileDist": "NODE_ENV='production' webpack --optimize-minimize",
    "serveDist": "NODE_ENV='production' node_modules/.bin/http-server dist",
    "serveDocs": "NODE_ENV='production' node_modules/.bin/http-server ../../../docs",
    "deployDist": "rm -rf ../../../docs && cp -R dist/. ../../../docs"
  },
  "author": "Andrew Allbright",
  "repository": {
    "type": "git",
    "url": "git://github.com/aallbrig/carpe-noctem.git"
