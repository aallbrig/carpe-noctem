{
  "rules": {
    "quotemark": [true, "single"],
    "indent": [true, "spaces"],
    "member-access": [true, "check-accessor"],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "semicolon": [true, "always"]
  }
}